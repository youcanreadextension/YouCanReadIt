{
  "name": "YOU CAN READ IT Dyslexia won't stop you!",
  "description": "By splitting up long words visibly, this extension helps people with their reading!",
  "version": "0.1.1.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/YouCanReadIt_12.png",
      "32": "/images/YouCanReadIt_32.png",
      "48": "/images/YouCanReadIt_48.png",
      "128": "/images/YouCanReadIt_128.png"
    }
  },
  "icons": {
    "16": "/images/YouCanReadIt_12.png",
    "32": "/images/YouCanReadIt_32.png",
    "48": "/images/YouCanReadIt_48.png",
    "128": "/images/YouCanReadIt_128.png"
  },
  "options_page": "options.html"
}
